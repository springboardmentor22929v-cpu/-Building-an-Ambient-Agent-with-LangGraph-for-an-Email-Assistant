# --- CONTROL PANEL ---
# Set ONLY ONE to "true"
USE_GEMINI=false
USE_GROQ=true

# --- MODEL NAMES ---
GEMINI_MODEL="gemini-2.5-flash-lite"
GROQ_MODEL="llama-3.3-70b-versatile" 

# --- API KEYS (Add as many as you want) ---
# The system will rotate through these if one fails.

# Google Keys
GOOGLE_API_KEY_1="your-google-api-key-here"
# GOOGLE_API_KEY_2="your-backup-google-api-key-here"
# GOOGLE_API_KEY_3="your-third-google-api-key-here"

# Groq Keys
GROQ_API_KEY_1="your-groq-api-key-here"
# GROQ_API_KEY_2="your-backup-groq-api-key-here"
# GROQ_API_KEY_3="your-third-groq-api-key-here"

# Set to 'true' to use Real Gmail. Set to 'false' for testing.
USE_REAL_TOOLS=true

# LangSmith (Keep this for debugging)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY="your-langchain-api-key-here"
LANGCHAIN_ENDPOINT="https://api.smith.langchain.com"
LANGCHAIN_PROJECT="ambient-agent-v1"